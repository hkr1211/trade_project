{
  "builds": [
    {
      "src": "trade_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "run_migrate.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src":"server.py",
      "use":"@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "^/run-migrate$",
      "dest": "/run_migrate.py"
    },
    {
      "src":"/(.*)",
      "dest":"server.py"
    },
    {
      "src": "/(.*)",
      "dest": "trade_project/wsgi.py"
    }
  ],
  "buildCommand": "pybabel compile -d locale && pip install -r requirements.txt && python manage.py collectstatic --noinput"
}