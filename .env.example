# Django 配置
SECRET_KEY=your-secret-key-here
DEBUG=False

# 数据库配置 (Supabase)
# 格式: postgresql://[user]:[password]@[host]:[port]/[database]
# 重要: 使用 Supabase 的连接池端口 6543，而不是直接连接端口 5432
# 示例: postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:6543/postgres
DATABASE_URL=postgresql://postgres:your-password@db.xxxxx.supabase.co:6543/postgres

# Vercel 环境变量（自动设置）
# VERCEL=1
# VERCEL_ENV=production
